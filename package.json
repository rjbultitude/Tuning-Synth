    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.4.2",
    "browserslist": "^4.8.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
